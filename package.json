{
  "name": "dialog-label",
  "version": "1.0.1",
  "description": "Dialog System NLU data label (web based on https://github.com/vikpe/react-webpack-typescript-starter)",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
